{
"manifest_version": 2,
	"name": "Favorites for Twitter",
	"version": "1.0",
	"author": "Corbin Davenport",
	"description": "Favorites for Twitter brings the old star button back to Twitter.",
	"web_accessible_resources": [
		"img/*"
	],
	"content_scripts": [
		{
		"matches": ["https://twitter.com/*"],
		"css": ["styles.css"],
		"run_at": "document_start"
		}
	],
	"icons":{
		"16":"img/icon16.png",
		"32":"img/icon32.png",
		"48":"img/icon48.png",
		"128":"img/icon128.png"
	}
}
